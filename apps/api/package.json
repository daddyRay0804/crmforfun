{
  "name": "@bet-crm/api",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main.js",
    "lint": "eslint .",
    "test": "jest",
    "migrate:up": "node-pg-migrate up -m migrations -d DATABASE_URL",
    "migrate:down": "node-pg-migrate down -m migrations -d DATABASE_URL",
    "migrate:create": "node-pg-migrate create -m migrations",
    "seed:admin": "ts-node -T src/seed/seed-admin.ts",
    "seed:demo": "ts-node -T src/seed/seed-demo.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.1",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^8.0.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
